<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HalamanLogbookActivity"
    android:background="@drawable/bgrn">

    <TextView
        android:id="@+id/textLogbook"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="140dp"
        android:fontFamily="@font/poppins_medium"
        android:text="Logbook"
        android:textColor="@color/white"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvlogbook"
        android:layout_width="413dp"
        android:layout_height="617dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="6dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.222"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textLogbook"
        tools:listitem="@layout/item_seminar" />

    <ImageView
        android:id="@+id/backk"
        android:layout_width="66dp"
        android:layout_height="32dp"
        android:layout_marginEnd="44dp"
        android:onClick="home"
        app:layout_constraintBottom_toBottomOf="@+id/textLogbook"
        app:layout_constraintEnd_toStartOf="@+id/textLogbook"
        app:layout_constraintTop_toTopOf="@+id/textLogbook"
        app:layout_constraintVertical_bias="0.333"
        app:srcCompat="@drawable/ic_backkk" />

</androidx.constraintlayout.widget.ConstraintLayout>
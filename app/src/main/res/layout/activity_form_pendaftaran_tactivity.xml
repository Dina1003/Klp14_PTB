<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FormPendaftaranTActivity">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="465dp"
        android:layout_height="101dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/yunen" />

    <TextView
        android:id="@+id/judul"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/poppins_medium"
        android:text="FORM PENDAFTARAN TUGAS AKHIR"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <EditText
        android:id="@+id/nama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="@drawable/bginput"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Nama Mahasiswa"
        app:layout_constraintEnd_toEndOf="@+id/judul"
        app:layout_constraintHorizontal_bias="0.212"
        app:layout_constraintStart_toStartOf="@+id/judul"
        app:layout_constraintTop_toBottomOf="@+id/judul" />

    <EditText
        android:id="@+id/nim"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:background="@drawable/bginput"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="NIM"
        app:layout_constraintEnd_toEndOf="@+id/nama"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/nama"
        app:layout_constraintTop_toBottomOf="@+id/nama" />

    <EditText
        android:id="@+id/notelp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:background="@drawable/bginput"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Nomor Telepon"
        app:layout_constraintEnd_toEndOf="@+id/nim"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/nim"
        app:layout_constraintTop_toBottomOf="@+id/nim" />

    <EditText
        android:id="@+id/prodi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:background="@drawable/bginput"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Prodi"
        app:layout_constraintEnd_toEndOf="@+id/notelp"
        app:layout_constraintStart_toStartOf="@+id/notelp"
        app:layout_constraintTop_toBottomOf="@+id/notelp" />

    <EditText
        android:id="@+id/pembimbing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="@drawable/bginput"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Pembimbing"
        app:layout_constraintEnd_toEndOf="@+id/prodi"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/prodi"
        app:layout_constraintTop_toBottomOf="@+id/prodi" />

    <EditText
        android:id="@+id/judulta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:background="@drawable/bginput"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Judul TA"
        app:layout_constraintEnd_toEndOf="@+id/pembimbing"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/pembimbing"
        app:layout_constraintTop_toBottomOf="@+id/pembimbing" />

    <EditText
        android:id="@+id/mulai"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="@drawable/bginput"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Mulai Dikerjakan"
        app:layout_constraintEnd_toEndOf="@+id/judulta"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/judulta"
        app:layout_constraintTop_toBottomOf="@+id/judulta" />

    <ImageButton
        android:id="@+id/daftar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:backgroundTint="#00FFFFFF"
        android:contentDescription="daftar"
        android:onClick="daftar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mulai"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/dftar" />

</androidx.constraintlayout.widget.ConstraintLayout>
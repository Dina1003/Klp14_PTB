<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFFFF"
    tools:context=".DetailSidang">

    <ImageView
        android:id="@+id/Head"
        android:layout_width="406dp"
        android:layout_height="95dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_background_atas" />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="53dp"
        android:layout_height="57dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/Head"
        app:srcCompat="@drawable/logo_unand" />


    <ImageView
        android:id="@+id/back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:onClick="kembali"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Head"
        app:srcCompat="@drawable/ic_back"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/text_jadwal"
        android:layout_width="337dp"
        android:layout_height="35dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/text_kostum"
        android:text="  Jum'at/ 21-10-2022 "
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Sidang_text" />

    <TextView
        android:id="@+id/file_report"
        android:layout_width="337dp"
        android:layout_height="35dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="80dp"
        android:background="@drawable/text_kostum"
        android:text="  Jum'at/ 21-10-2022 "
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Sidang_text" />

    <TextView
        android:id="@+id/file_slide"
        android:layout_width="337dp"
        android:layout_height="35dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="120dp"
        android:background="@drawable/text_kostum"
        android:text="  Jum'at/ 21-10-2022 "
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Sidang_text" />

    <TextView
        android:id="@+id/file_journal"
        android:layout_width="337dp"
        android:layout_height="35dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="160dp"
        android:background="@drawable/text_kostum"
        android:text="  Jum'at/ 21-10-2022 "
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Sidang_text" />

    <TextView
        android:id="@+id/startend_at"
        android:layout_width="337dp"
        android:layout_height="35dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="200dp"
        android:background="@drawable/text_kostum"
        android:text="  Jum'at/ 21-10-2022 "
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Sidang_text" />

    <TextView
        android:id="@+id/room_id"
        android:layout_width="337dp"
        android:layout_height="35dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="240dp"
        android:background="@drawable/text_kostum"
        android:text="  Jum'at/ 21-10-2022 "
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Sidang_text" />

    <TextView
        android:id="@+id/online_url"
        android:layout_width="337dp"
        android:layout_height="35dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="280dp"
        android:background="@drawable/text_kostum"
        android:text="  Jum'at/ 21-10-2022 "
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Sidang_text" />

    <TextView
        android:id="@+id/score"
        android:layout_width="337dp"
        android:layout_height="35dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="320dp"
        android:background="@drawable/text_kostum"
        android:text="  Jum'at/ 21-10-2022 "
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Sidang_text" />

    <TextView
        android:id="@+id/grade"
        android:layout_width="337dp"
        android:layout_height="35dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="360dp"
        android:background="@drawable/text_kostum"
        android:text="  Jum'at/ 21-10-2022 "
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Sidang_text" />

    <ImageView
        android:id="@+id/Sidang_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Head"
        app:srcCompat="@drawable/ic_teks_sidangakhir"
        />

    <Button
        android:id="@+id/btnNotif"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="Notifikasi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rvpenguji" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvpenguji"
        android:layout_width="309dp"
        android:layout_height="100dp"
        android:layout_marginTop="30dp"
        app:layout_constraintStart_toStartOf="@+id/grade"
        app:layout_constraintTop_toBottomOf="@+id/grade" />


</androidx.constraintlayout.widget.ConstraintLayout>